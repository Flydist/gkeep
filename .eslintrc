{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb",
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "parser": "babel-eslint",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "plugins": [
    "react",
    "react-hooks",
    "prettier"
  ],
  "rules": {
    "react-hooks/rules-of-hooks": "warn",
    "react-hooks/exhaustive-deps": 0,
    "import/prefer-default-export": 0,
    "import/no-unresolved": 0,
    "react/state-in-constructor": 0,
    "react/jsx-wrap-multilines": [
      "error",
      {
        "declaration": false,
        "assignment": false
      }
    ],
    "react/no-unused-prop-types": 0,
    "react/require-default-props": 0,
    "react/jsx-props-no-spreading": "off",
    "react/display-name": 0,
    "react/no-array-index-key": 0,
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ]
      }
    ],
    "semi": [
      "warn",
      "never"
    ],
    "no-console": 0,
    "no-unused-vars": [
      "warn",
      {
        "vars": "local"
      }
    ],
    "linebreak-style": 0,
    "comma-dangle": [
      "error",
      {
        "objects": "never"
      }
    ],
    "arrow-parens": 0,
    "jsx-quotes": [
      "error",
      "prefer-single"
    ],
    "arrow-body-style": 0,
    "eol-last": 0,
    "consistent-return": 0
  }
}